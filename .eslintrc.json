{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true  
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      2,
      "double"
    ],
    " no-duplicate-imports": "0",
    "no-var":2,
    "no-useless-constructor": 2,
    "newline-before-return": 0,
    "lines-around-directive": [
      0,
      "always"
    ],
    "inline-break-line": 0,
    "no-multi-assign":0,
    "linebreak-style": 0,
    "camelcase":0
   
  }
}
