{
  "name": "api",
  "version": "1.0.0",
  "description": "Easy optimization of data",
  "main": "app.js",
  "scripts": {
    "build": "babel ./server -d build",
    "test": "nyc mocha build/unitTest/*.js --exit",
    "devTest": "cross-env NODE_ENV=test nyc --require @babel/register  mocha server/unitTest --exit",
    "singleTest": "cross-env NODE_ENV=test nyc --require @babel/register  mocha --exit",
    "create": "node ./server/database/database.main.js",
    "fullTable": "node ./server/database/fullTable.js",
    "startBabel": "babel ./server -d build && node build/app.js",
    "start": "node ./build/app.js",
    "dev": "nodemon --exec babel-node server/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "email": "babel-node server/helpers/mail.js"
  },
  "engines": {
    "node": "v11.8.0"
  },
  "author": "Dike Chinenye <nennydike@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.2.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-generator": "^4.16.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "nodemailer": "^6.1.1",
    "pg": "^7.10.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "expect": "^24.7.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.10",
    "nyc": "^14.0.0"
  }
}
