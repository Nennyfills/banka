{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "single"],
    " no-duplicate-imports": "error",
    "no-var": "error",
    "no-useless-constructor": "error",
   "newline-before-return": "error",
  "lines-around-directive":["error", "always"]
  }
}
